    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^5.0.3",
    "date-fns": "^1.30.1",
    "express": "^4.16.4",
    "fs": "0.0.1-security",
