    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-watch": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "ignore-loader": "^0.1.2",
    "jest": "^24.1.0",
    "jest-dom": "^3.0.1",
    "jest-prop-type-error": "^1.1.0",
    "mini-css-extract-plugin": "^0.5.0",
