  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf build && NODE_ENV=production webpack --mode production",
    "dev": "rm -rf build && NODE_ENV=development webpack --mode development",
    "dev:watch": "NODE_ENV=development webpack --mode development -w",
    "start": "npm run dev && concurrently \"npm run dev:watch\" \"NODE_ENV=development APP_PATH=relative nodemon build/server.js\"",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "prettier": "prettier --check '**/**.js'",
    "prettier:fix": "prettier --check '**/**.js' --write",
    "sasslint": "sass-lint -c .sass-lint.yml -v -q",
    "sasslint:watch": "esw -w src/**",
    "test": "jest --watch",
    "test:ci": "jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u"
  },
  "dependencies": {
    "@loadable/component": "^5.2.2",
    "@reach/router": "^1.2.1",
    "amat-react": "^0.1.10",
